/* MarkDown Style */
@import '_syntax'
@import '_highlight_vs'

blockquote
	margin: 0
	padding: 10px 15px
	color: #444
	border-left: 4px solid #ccc

pre .line
	font-family: "Source Code Pro", Consolas, Monaco, Menlo, monospace
	height: 1.6em
	font-size: 15px

code
	font-family: Menlo,Monaco,Consolas,"Courier New",monospace
	padding: 2px 4px
	font-size: 90%
	color: #c7254e
	background-color: #f9f2f4
	border-radius: 4px
